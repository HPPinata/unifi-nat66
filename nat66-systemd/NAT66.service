[Unit]
Description=Enable NAT66 on Startup
Wants=network-online.target
After=network-online.target

[Service]
Type=oneshot
ExecStart=bash -c 'ip6tables -t nat -A POSTROUTING -s fd00::0/8 -j MASQUERADE'
ExecStop=bash -c 'ip6tables -t nat -D POSTROUTING -s fd00::0/8 -j MASQUERADE'
RemainAfterExit=true

[Install]
WantedBy=multi-user.target
